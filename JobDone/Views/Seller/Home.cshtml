@{
    Layout = "~/Views/Shared/_SellerLayout.cshtml";
}





<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Sparlex - Spa Website Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=PT+Serif:wght@400;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="seller/lib/animate/animate.min.css" rel="stylesheet">
    <link href="seller/lib/lightbox/css/lightbox.min.css" rel="stylesheet">
    <link href="seller/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">


    <!-- Customized Bootstrap Stylesheet -->
    <link href="seller/css/boootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="seller/css/style.css" rel="stylesheet">





    <!-- Libraries Stylesheet -->
    <link href="seller/liib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="seller/liib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="sellercss/booootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="seller/css/sstyle.css" rel="stylesheet">





</head>

<body>

    <br /><br />
    <div class="container-fluid carousel-header px-0">
        <div id="carouselId" class="carousel slide" data-bs-ride="carousel">
            <ol class="carousel-indicators">
                <li data-bs-target="#carouselId" data-bs-slide-to="0" class="active"></li>
                <li data-bs-target="#carouselId" data-bs-slide-to="1"></li>
                <li data-bs-target="#carouselId" data-bs-slide-to="2"></li>
            </ol>
            <div class="carousel-inner" role="listbox">
                <div class="carousel-item active">
                    <img src="~/Seller/img/banner1.jpg" style="width:100%;" class="img-fluid" alt="Image">
                    <div class="carousel-caption">
                        <div class="p-3" style="max-width: 900px;">
                            <h4 class="text-primary text-uppercase mb-3">
                                <h1 class="display-1 text-capitalize text-dark mb-3">
                                    <p class="mx-md-5 fs-4 px-4 mb-5 text-dark">
                                        
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/Seller/img/banner2.jpg" style="width:100%;" class="img-fluid" alt="Image">
                    <div class="carousel-caption">
                        <div class="p-3" style="max-width: 900px;">
                            <h4 class="text-primary text-uppercase mb-3" style="letter-spacing: 3px;">
                                <h1 class="display-1 text-capitalize text-dark mb-3">
                                    <p class="mx-md-5 fs-4 px-5 mb-5 text-dark">
                                        
                        </div>
                    </div>
                </div>

                <div class="carousel-item">
                    <img src="~/Seller/img/banner3.jpg" style="width:100%;" class="img-fluid" alt="Image">
                    <div class="carousel-caption">
                        <div class="p-3" style="max-width: 900px;">
                            <h4 class="text-primary text-uppercase mb-3" style="letter-spacing: 3px;">
                                <h1 class="display-1 text-capitalize text-dark">
                                    <p class="mx-md-5 fs-4 px-5 mb-5 text-dark">
                                       
                        </div>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselId" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselId" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>  </div>

    <!-- Sale & Revenue Start -->
    <div class="container-fluid pt-4 px-4">
        <div class="row g-4">
            <div class="col-sm-6 col-xl-3">
                <div class="bg-light rounded d-flex align-items-center justify-content-between p-4">
                    <i class="fa fa-chart-line fa-3x text-primary"></i>
                    <div class="ms-3">
                        <p class="mb-2">Today Sale</p>
                        <h6 class="mb-0">$1234</h6>
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-xl-3">
                <div class="bg-light rounded d-flex align-items-center justify-content-between p-4">
                    <i class="fa fa-chart-bar fa-3x text-primary"></i>
                    <div class="ms-3">
                        <p class="mb-2">Remining Work</p>
                        <h6 class="mb-0">@ViewBag.xxx</h6>
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-xl-3">
                <div class="bg-light rounded d-flex align-items-center justify-content-between p-4">
                    <i class="fa fa-chart-area fa-3x text-primary"></i>
                    <div class="ms-3">
                        <p class="mb-2">Available Reqest</p>
                        <h6 class="mb-0">@ViewBag.aveilabelReqest</h6>
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-xl-3">
                <div class="bg-light rounded d-flex align-items-center justify-content-between p-4">
                    <i class="fa fa-chart-pie fa-3x text-primary"></i>
                    <div class="ms-3">
                        <p class="mb-2">Total Revenue</p>
                        <h6 class="mb-0">@Math.Round(ViewBag.Totalgains,2)$</h6>
                    </div>
                </div>
            </div>
        </div>
        <br />
    <!-- Sale & Revenue End -->


    <!-- Sales Chart Start -->
    <div class="container-fluid pt-4 px-4">
        <div class="row g-4">
            <div class="col-sm-12 col-xl-6">
                <div class="bg-light text-center rounded p-4">
                    <div class="d-flex align-items-center justify-content-between mb-4">
                            <h6 class="mb-0">Remining Work</h6>
                            <a asp-controller="Seller" asp-action="Order">Show All</a>
                    </div>
                    <canvas id="worldwide-sales"></canvas>
                </div>
            </div>
            <div class="col-sm-12 col-xl-6">
                <div class="bg-light text-center rounded p-4">
                    <div class="d-flex align-items-center justify-content-between mb-4">
                            <h6 class="mb-0">aveilabel Reqest</h6>
                            <a asp-controller="Seller" asp-action="RequestedWrok">Show All</a>
                    </div>
                    <canvas id="salse-revenue"></canvas>
                </div>
            </div>
        </div>
    </div>
    <!-- Sales Chart End -->











    </div>
    <!-- JavaScript Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="seller/lib/wow/wow.min.js"></script>
    <script src="seller/lib/easing/easing.min.js"></script>
    <script src="seller/lib/waypoints/waypoints.min.js"></script>
    <script src="seller/lib/counterup/counterup.min.js"></script>
    <script src="seller/lib/lightbox/js/lightbox.min.js"></script>
    <script src="seller/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="seller/js/maain.js"></script>


    <script src="seller/liib/chart/chart.min.js"></script>
    <script src="seller/liib/easing/easing.min.js"></script>
    <script src="seller/liib/waypoints/waypoints.min.js"></script>
    <script src="seller/liib/owlcarousel/owl.carousel.min.js"></script>
    <script src="seller/liib/tempusdominus/js/moment.min.js"></script>
    <script src="seller/liib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="seller/liib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="seller/js/maain.js"></script>



</body>

</html>